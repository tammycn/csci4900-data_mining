-- POSS OF MARIJ
-- ------------------------------
-- POSS OF MARIJ-1OZ-
-- POSS MARIJ W/INTENT
-- POSS/MANU/SELL/MARIJ
UPDATE booking_charge SET charge_category='POSS OF MARIJ'
WHERE charge_description IN ('POSS OF MARIJ-1OZ-', 'POSS MARIJ W/INTENT', 'POSS/MANU/SELL/MARIJ');

-- DUI
-- ------------------------------
-- DUI ALCOHOL
-- DUI DRUGS
-- DUI ENDANGER CHILD
-- DUI MULTIPLE SUBS
-- POSS OPEN CONT VEHIC
UPDATE booking_charge SET charge_category='DUI' WHERE charge_description LIKE '%dui%';
UPDATE booking_charge SET charge_category='DUI'
WHERE charge_description IN ('POSS OPEN CONT VEHIC');

-- POSS SCH CONT SUB
